                activity2Title: 'Proyecto completado',
                activity2Desc: 'Mar√≠a Gonz√°lez - Inspecci√≥n final aprobada',
                activity4Title: 'Materiales recibidos',
                activity4Desc: 'Gabinetes y accesorios para proyecto'
            }
        };

        // Update category display and terminology
        function updateCategory() {
            const select = document.getElementById('companyCategory');
            const category = CATEGORIES[select.value];
            
            // Update icon and display
            document.getElementById('categoryIcon').textContent = category.icon;
            document.getElementById('categoryDisplay').textContent = category.name;
            
            // Update stat labels
            document.getElementById('statLabel1').textContent = category.statLabel1;
            document.getElementById('statLabel2').textContent = category.statLabel2;
            
            // Update action buttons
            document.getElementById('actionNew').textContent = category.actionNew;
            document.getElementById('actionInventory').textContent = category.actionInventory;
            
            // Update team roles
            document.getElementById('teamRole1').textContent = category.teamRole1;
            document.getElementById('teamRole2').textContent = category.teamRole2;
            document.getElementById('teamRole3').textContent = category.teamRole3;
            document.getElementById('teamRole4').textContent = category.teamRole4;
            document.getElementById('teamRole5').textContent = category.teamRole5;
            
            // Update activity descriptions
            document.getElementById('activity1Title').textContent = category.activity1Title;
            document.getElementById('activity1Desc').textContent = category.activity1Desc;
            document.getElementById('activity2Title').textContent = category.activity2Title;
            document.getElementById('activity2Desc').textContent = category.activity2Desc;
            document.getElementById('activity4Title').textContent = category.activity4Title;
            document.getElementById('activity4Desc').textContent = category.activity4Desc;
            
            // Save preference
            localStorage.setItem('opsisCategory', select.value);
        }

        // Load saved category on page load
        window.addEventListener('DOMContentLoaded', () => {
            const savedCategory = localStorage.getItem('opsisCategory');
            if (savedCategory) {
                document.getElementById('companyCategory').value = savedCategory;
            }
            updateCategory();
        });
    </script>
</body>
</html>
